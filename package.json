  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack/webpack.config.js --env env=prod --mode production --json build/webpack-stats.json",
    "format": "prettier --write --config ./.prettierrc.js \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
