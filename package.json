    "lint": "eslint --fix --color --cache \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "lint:ci": "eslint \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "start": "webpack serve --config webpack/webpack.config.js --env env=dev",
    "server": "npx serve ./build",
    "test": "jest --watch"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "react": "^18.0.0",
